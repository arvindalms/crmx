<h1 >New Organization</h1>
<br/>
<%=form_for @organization, url: organizations_path, method: :post do |f|%>
	<table class="table" style="width:100px !important">
		<tr>
			<th><%=f.label :name%></th>
			<td><%=f.text_field :name%></td>
		</tr>
		<tr>
			<th></th>
			<td><%=f.submit :class=>"btn btn-success" %></td>
		</tr>
	</table>
<%end%>
